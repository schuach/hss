    <marc:datafield tag="245" ind1="0" ind2="0">
      <marc:subfield code="a">Die Bedeutung von Selbstmitgefühl und Trait Emotionaler Intelligenz für Wohlbefinden und Stress</marc:subfield>
    </marc:datafield>
    <marc:datafield tag="246" ind1="3" ind2="3">
      <marc:subfield code="a">The relevance of self-compassion and trait emotional intelligence for well-being and stress</marc:subfield>
    </marc:datafield>
